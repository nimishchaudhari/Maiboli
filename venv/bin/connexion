#!/Users/nimishchaudhari/maiboli/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'connexion==2.2.0','console_scripts','connexion'
__requires__ = 'connexion==2.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('connexion==2.2.0', 'console_scripts', 'connexion')()
    )
